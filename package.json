{
  "name": "nyc-civic-visualization",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.0",
  "scripts": {
    "dev": "node --env-file=.env node_modules/.bin/vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check:duplicates": "node --env-file=.env scripts/check-duplicate-locations.js",
    "check:specifics": "node --env-file=.env scripts/check-specific-addresses.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate-housing": "node --env-file=.env scripts/apply-housing-migration.js",
    "seed:housing": "node --env-file=.env scripts/seed-housing.js",
    "seed:capital": "node --env-file=.env scripts/seed-capital-budget.js",
    "seed:financial": "node --env-file=.env scripts/seed-financial.js",
    "seed:all": "bun run seed:housing && bun run seed:capital && bun run seed:financial",
    "verify:housing": "node --env-file=.env scripts/verify-housing-quick.js",
    "explore:dcp-housing": "node scripts/explore-dcp-housing.js"
  },
  "dependencies": {
    "@deck.gl/core": "^9.2.2",
    "@deck.gl/layers": "^9.2.2",
    "@deck.gl/react": "^9.2.2",
    "@types/cytoscape": "^3.31.0",
    "clsx": "^2.1.1",
    "cytoscape": "^3.33.1",
    "cytoscape-elk": "^2.3.0",
    "cytoscape-klay": "^3.1.4",
    "d3-hierarchy": "^3.1.2",
    "d3-interpolate": "^3.0.1",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "elkjs": "^0.11.0",
    "maplibre-gl": "^5.11.0",
    "postgres": "^3.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-map-gl": "^8.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/connect": "^3.4.38",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-sankey": "^0.12.4",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-scale-chromatic": "^3.1.0",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-transition": "^3.0.9",
    "@types/maplibre-gl": "^1.13.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "connect": "^3.7.0",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
