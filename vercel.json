{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin/public/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/admin/public/index.html"
    },
    {
      "source": "/admin/app.js",
      "destination": "/admin/public/app.js"
    }
  ],
  "functions": {
    "admin/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
